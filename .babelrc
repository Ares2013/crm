{
    "presets": [
        ["env", {
            "targets": {
              "browsers": ["last 2 versions", "safari >= 7"]
            }
        }],
        "react",
        "stage-0"
    ],
    "plugins": [
        ["transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }],
        "react-hot-loader/babel",
        "transform-decorators-legacy",
        ["import",{
            "libraryName": "antd",
            "style": "css"
        }]
    ]
}